<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.qinguangfeng.mapper" auto-import="true">
	<class name="com.qinguangfeng.entity.User" table="tb_user">
		<id name="uId">
			<generator class="native"></generator>
		</id>
		<property name="userName" column="user_name"></property>
		<property name="pwd" column="user_pwd"></property>
	</class>
</hibernate-mapping>
